{
  "name": "rxui",
  "version": "0.1.0",
  "description": "A MVVM framework that integrates Reactive Extensions for JavaScript. Inspired by ReactiveUI for .Net.",
  "scripts": {
    "build:w": "tsc -w",
    "build": "tsc",
    "test": "npm run build && npm run mocha",
    "mocha": "mocha dist/cjs/test",
    "mocha:w": "mocha -w dist/cjs/test",
    "test:w": "concurrent \"npm run build:w\" \"npm run mocha:w\""
  },
  "keywords": [
    "ReactiveX",
    "RxJS",
    "Rx",
    "Observables",
    "ReactiveExtensions",
    "MVVM"
  ],
  "author": "Kallyn Gowdy <kallyngowdy@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "rxjs": "^5.0.0-beta.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "^2.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^2.4.5",
    "typescript": "^1.8.7"
  }
}
